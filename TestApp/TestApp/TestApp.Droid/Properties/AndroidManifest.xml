<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.rosva.testapp" android:installLocation="auto">
	<uses-sdk android:minSdkVersion="21" />

  <!-- We need to be able to download map tiles and access Google Play Services-->
  <uses-permission android:name="android.permission.INTERNET" />
  <!-- Allow the application to access Google web-based services. -->
  <!-- Google Maps for Android v2 will cache map tiles on external storage -->
  <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
  <!-- Google Maps for Android v2 needs this permission so that it may check the connection state as it must download data -->
  <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
  <!-- These are optional, but recommended. They will allow Maps to use the My Location provider. -->
  <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
  <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
  <!-- Allow the application to access Google web-based services. -->
  <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
  
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
  <permission
    android:name="com.rosva.testapp.droid.permission.MAPS_RECEIVE"
    android:protectionLevel="signature"/>
  <uses-permission android:name="com.rosva.testapp.droid.permission.MAPS_RECEIVE"/>

  <application
    android:label="TestApp.Droid"
    android:theme="@style/AppTheme">
    
    <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyCecqOPad5P4-qIy31rBhFhYx73cmPR3ro" />
    

